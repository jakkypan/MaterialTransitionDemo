<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.giant.materialtransition">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".FadeActivity" />
        <activity android:name=".SlideActivity" />
        <activity android:name=".ExplodeActivity" />
        <activity android:name=".ChangeBoundsActivity" />
        <activity android:name=".ChangeTransformActivity" />
        <activity android:name=".ChangeClipBoundsActivity" />
        <activity android:name=".ChangeClipBoundsActivity2" />
        <activity android:name=".ComplexActivity" />

    </application>

</manifest>